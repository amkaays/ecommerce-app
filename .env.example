# File: .env.example

# Express Session Secret. Please use a strong and unique key.
SESSION_SECRET=a_very_strong_and_long_secret_key

# Google OAuth 2.0 Credentials
# These are required for Google authentication in the application.
# Please replace the placeholders with your actual credentials.
# You can obtain these credentials from the Google Cloud Console: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK=http://localhost:3000/auth/google/callback